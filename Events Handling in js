    <div onmouseover="mOver(this)" onmouseout="mOut(this)" style="background-color: red; width: 120px; padding: 20px; ">
        Mouse Over me! 
    </div>
    <script>
        function display(obj){
            obj.innerHTML="This is updated list";
        }
        function btnDate(){
            document.getElementById("demo").innerHTML=new Date;
        }
        function mOver(obj) {
            obj.innerHTML="Thanks Bro";
        }
        function mOut(obj) {
            obj.innerHTML="Mouse Over please";
        }
    </script>
